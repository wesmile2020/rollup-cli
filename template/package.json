{
  "name": "rollup-template",
  "version": "1.0.0",
  "description": "rollup template",
  "main": "src/index.ts",
  "scripts": {
    "start": "rollup -c -m -w --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "build-node": "rollup -c --environment ESM,NODE_ENV:production",
    "doc": "node build/doc.js",
    "lint": "eslint ./src --ext .ts"
  },
  "author": "lei.li",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "rollup": "^2.26.5",
    "rollup-plugin-lintes": "^1.0.0",
    "rollup-plugin-minize": "^1.1.0",
    "rollup-plugin-reload": "^2.7.0",
    "rollup-plugin-worker": "^2.2.0",
    "tslib": "^2.0.1",
    "typedoc": "^0.18.0",
    "typescript": "^4.0.2"
  }
}
